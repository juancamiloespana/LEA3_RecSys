      "source": [
        "\n",
        "#############################################\n",
        "#### conectar_base_de_Datos#################\n",
        "############################################\n",
        "\n",
        "conn=sql.connect('data/db_books2')\n",
        "cur=conn.cursor()\n",
        "\n",
        "cur.execute(\"SELECT name FROM sqlite_master WHERE type='table';\")\n",
        "print(cur.fetchall())\n"
